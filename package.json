{
  "name": "hostel-management",
  "version": "1.0.0",
  "description": "Login Portal",
  "main": "server.js",
  "scripts": {
    "test": "jest __tests__ --forceExit",
    "test:watch": "jest --watchAll --forceExit",
    "client-install": "npm install --prefix client",
    "start": "nodemon server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starkblaze01/Hostel-Management.git"
  },
  "author": "Mayank Pathela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/starkblaze01/Hostel-Management/issues"
  },
  "homepage": "https://github.com/starkblaze01/Hostel-Management#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^5.3.0",
    "express": "^4.16.4",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.13",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "nodemon": "^1.19.4",
    "supertest": "^6.1.6"
  }
